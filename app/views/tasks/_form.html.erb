<div class="container mt-3">
  <%= form_with model: @task do |f| %>
    <%= f.label :title %>
    <%= f.text_field :title %>
    <%= f.label :details %>
    <%= f.text_field :details %>
    <%= f.check_box :completed %>
    <%= f.label :completed %>
    <%= f.submit type="Create task" %>
  <% end %>
</div>
